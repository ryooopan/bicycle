
<script type="text/javascript" charset="utf-8">
$(function () {
  new Highcharts.Chart({
    chart: { renderTo: 'graph' },
    title: { text: 'Power' },
    xAxis: { type: 'Time' },
    yAxis: {
      title: { text: 'Wat'}
    },
    series: [{
      name: "Power",
      pointInterval: <%= 1.day * 1000 %>,
      pointStart: <%= 3.weeks.ago.at_midnight.to_i * 1000 %>,
      data: <%= @power %>
    },
    {
      name: "Battery",
      pointInterval: <%= 1.day * 1000 %>,
      pointStart: <%= 3.weeks.ago.at_midnight.to_i * 1000 %>,
      data: <%= @battery %>
    }]
  });
});
</script>

<div id="graph" style="width: 100%;"></div>

<%= @a %>

<% @power.each do |p| %>
<h1><%= p %></h1>
<% end %>

<% @battery.each do |b| %>
<h1><%= b %></h1>
<% end %>





